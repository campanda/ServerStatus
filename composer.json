{
    "name": "campanda/server-status",
    "type": "library",
    "description": "Library to access various components of the operating system",
    "homepage": "http://github.com/campanda/server-status",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        },
        {
            "name": "Helmut Hoffer von Ankershoffen",
            "email": "hhva@campanda.com"
        }
    ],
    "support": {
        "issues": "http://github.com/campanda/server-status/issues"
    },
    "require": {
        "php": "~7.1",
        "innmind/immutable": "~2.3",
        "innmind/time-continuum": "^1.0",
        "symfony/process": "^3.2|~4.0"
    },
    "autoload": {
        "psr-4": {
            "Campanda\\Server\\Status\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Campanda\\Server\\Status\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    }
}
